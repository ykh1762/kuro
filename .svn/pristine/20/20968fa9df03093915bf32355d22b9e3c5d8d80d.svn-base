<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>임시 페이지-나중에 삭제할 예정</title>
</head>
<body>
	<div class="scrollable position-relative" style="height: 100%">
            <!-- ---------------------
                        start Card example
                    ---------------- -->
            <div class="p-3">
              <h5 class="card-title">Card example</h5>
              <img src="../../assets/images/users/2.jpg" class="img-fluid" alt="users" />
              <p class="mt-2">
                This is example text to build on the card title and make up the bulk of the card's
                content.
              </p>
              <button type="button" class="btn waves-effect waves-light btn-info">Info</button>
            </div>
            <!-- ---------------------
                        end Card example
                    ---------------- -->
            <hr />
            <!-- ---------------------
                        start Form example
                    ---------------- -->
            <div class="p-3">
              <h5 class="card-title">Form example</h5>
              <form class="mt-3">
                <div class="mb-3">
                  <label for="exampleInputEmail1">Email address</label>
                  <input
                    type="email"
                    class="form-control"
                    id="exampleInputEmail1"
                    placeholder="Enter email"
                  />
                </div>
                <div class="mb-3">
                  <label for="exampleInputPassword1">Password</label>
                  <input
                    type="password"
                    class="form-control"
                    id="exampleInputPassword1"
                    placeholder="Password"
                  />
                </div>
                <div class="form-check mr-sm-2 mb-3">
                  <input type="checkbox" class="form-check-input" id="checkbox0" value="check" />
                  <label class="form-check-label" for="checkbox0">Check Me Out !</label>
                </div>
                <button type="submit" class="btn btn-success">Submit</button>
              </form>
            </div>
            <!-- ---------------------
                        end Form example
                    ---------------- -->
            <hr />
            <!-- ---------------------
                        start Recent News
                    ---------------- -->
            <div class="p-3">
              <h5 class="card-title">Recent News</h5>
              <ul class="list-unstyled mt-3">
                <li class="d-flex">
                  <a href=""
                    ><img
                      class="d-flex me-3"
                      src="../../assets/images/users/1.jpg"
                      width="60"
                      alt="Generic placeholder image"
                  /></a>
                  <div class="media-body">
                    <h6 class="mb-1">Here is the title you can add</h6>
                    <span>2 hours ago</span>
                  </div>
                </li>
                <li class="d-flex my-4">
                  <a href=""
                    ><img
                      class="d-flex me-3"
                      src="../../assets/images/users/2.jpg"
                      width="60"
                      alt="Generic placeholder image"
                  /></a>
                  <div class="media-body">
                    <h6 class="mb-1">Here is the title you can add</h6>
                    <span>3 hours ago</span>
                  </div>
                </li>
                <li class="d-flex">
                  <a href=""
                    ><img
                      class="d-flex me-3"
                      src="../../assets/images/users/4.jpg"
                      width="60"
                      alt="Generic placeholder image"
                  /></a>
                  <div class="media-body">
                    <h6 class="mb-1">Here is the title you can add</h6>
                    <span>4 hours ago</span>
                  </div>
                </li>
              </ul>
            </div>
            <!-- ---------------------
                        end Recent News
                    ---------------- -->
          </div>
</body>
</html>